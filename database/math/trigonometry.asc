sin(A+B) = sinAcosB + cosAsinB
sin(A-B) = sinAcosB - cosAsinB
cos(A+B) = cosAcosB - sinAsinB
cos(A-B) = cosAcosB + sinAsinB

tan(A+B) = (tanA+tanB)/(1-tanAtanB)
tan(A-B) = (tanA-tanB)/(1+tanAtanB)
cot(A+B) = (cotAcotB-1)/(cotB+cotA)
cot(A-B) = (cotAcotB+1)/(cotB-cotA)


sinC+sinD = 2sin((C+D)/2)cos((C-D)/2)
sinC-sinD = 2sin((C-D)/2)cos((C+D)/2)
cosC+cosC = 2cos((C+D)/2)cos((C-D)/2)
cosC-cosD = 2sin((C+D)/2)sin((D-C)/2)


2sinAcosB = sin(A+B)+sin(A-B)
2cosAsinB = sin(A+B)-sin(A-B)
2cosAcosB = cos(A+B)+cos(A-B)
2sinAsinB = cos(A-B)-sin(A+B)

sin2A = 2sinAcosA
cos2A = cos^2A - sin^2A
cos2A = 2cos^A - 1
cos2A = 1 - 2sin^2A
tan2A = (2tanA)/(1-tan^2A)
cos2A = (cot^2A-1)/(2cotA)
sin2A = (2tanA)/(1+tan^2A)

2cos^2A = 1 + cos2A
2sin^2A = 1 - cos2A

sin3A = 3sinA - 4sin^3A
cos3A = 4cos^3A - 3cosA

sin^3A = (3sinA - sin3A)/4
cos^3A = (3cosA + cos3A)/4

tan3A = (3tanA - tan^3A)/(1 - 3tan^2A)
cot3A = (3cotA - cot^3A)/(1 - 3cot^2A)
